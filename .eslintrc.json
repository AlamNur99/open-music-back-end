{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "indent": "off",
        "no-console": "off",
        "eol-last": "off",
        "camelcase": "off",
        "radix": "off"
    }
}